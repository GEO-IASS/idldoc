
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <title>[% sav_basefilename %] ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
         [% INCLUDE css_location %]
    </style>	
    <style type="text/css" media="print">
         [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all" href="[% root %]main_files.css" />
    <link rel="stylesheet" type="text/css" media="print" href="[% root %]main_files_print.css" />
    [% END %]
    
    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% sav_basefilename %] ([% title %])";
      }
    </script>
  </head>
    
  <body onload="setTitle();">
      
    [% INCLUDE_TEMPLATE navbar_filename %]
    
    <div id="container">

      <h1 class="directory"><a href="directory-overview.html">[% sav_dir %]</a></h1>      
      <h2 class="pro_file">[% sav_basefilename %]</h2>
      
      <table id="sav_attr" cellspacing="0">
	<tr>
	  <td>Description</td>
	  <td class="value">[% sav_description %]</td>
	</tr>
	<tr>
	  <td>Filetype</td>
	  <td class="value">[% sav_filetype %]</td>
	</tr>
	<tr>
	  <td>User</td>
	  <td class="value">[% sav_user %]</td>
	</tr>
	<tr>
	  <td>Host</td>
	  <td class="value">[% sav_host %]</td>
	</tr>
	<tr>
	  <td>Date</td>
	  <td class="value">[% sav_date %]</td>
	</tr>
	<tr>
	  <td>Architecture</td>
	  <td class="value">[% sav_arch %]</td>
	  </tr>
	<tr>
	  <td>Operating system</td>
	  <td class="value">[% sav_os %]</td>
	</tr>
	<tr>
	  <td>Release</td>
	  <td class="value">[% sav_release %]</td>
	</tr>
	[% IF ncommon gt 0 %]
	<tr>
	  <td><a href="#common">Common blocks</a></td>
	  <td class="value">[% ncommon %]</td>
	</tr>
	[% END %]
	[% IF nvar gt 0 %]
	<tr>
	  <td><a href="#var">Variables</a></td>
	  <td class="value">[% nvar %]</td>
	</tr>
	[% END %]
	[% IF nsystem gt 0 %]
	<tr>
	  <td><a href="#system">System variables</a></td>
	  <td class="value">[% nsystem %]</td>
	</tr>
	[% END %]
	[% IF nproc gt 0 %]
	<tr>
	  <td><a href="#proc">Procedures</a></td>
	  <td class="value">[% nproc %]</td>
	</tr>
	[% END %]
	[% IF nfunc gt 0 %]
	<tr>
	  <td><a href="#func">Functions</a></td>
	  <td class="value">[% nfunc %]</td>
	</tr>
	[% END %]
	[% IF nobj gt 0 %]
	<tr>
	  <td><a href="#obj">Objects</a></td>
	  <td class="value">[% nobj %]</td>
	</tr>
	[% END %]
	[% IF nptr gt 0 %]
	<tr>
	  <td><a href="#ptr">Pointers</a></td>
	  <td class="value">[% nptr %]</td>
	</tr>
	[% END %]
	[% IF nstructdef gt 0 %]
	<tr>
	  <td><a href="#structdef">Structure definitions</a></td>
	  <td class="value">[% nstructdef %]</td>
	</tr>
	[% END %]
      </table>

      [% IF ncommon gt 0 %] 
      <h2 id="common">[% ncommon %] common block variables</h2>
      <table class="variables">
      [% FOREACH c IN common_info %]
	<tr>
	  <td class="image"></td>
	  <td>[% c.declaration %]</td>
	</tr>
      [% END %]
      </table>
      [% END %]

      [% IF nvar gt 0 %] 
      <h2 id="var">[% nvar %] variable[% IF nvar gt 1 %]s[% END %]</h2>
      <table class="variables">
      [% FOREACH v IN var_info %]
	<tr>
	  <td class="image"><img src="[% v.filename %]"/></td>
	  <td>[% v.name %] = [% v.declaration %]</td>
	</tr>
      [% END %]
      </table>
      [% END %]

      [% IF nsystem gt 0 %] 
      <h2 id="system">[% nsystem %] system variable[% IF nsystem gt 1 %]s[% END %]</h2>
      <table class="variables">
      [% FOREACH s IN system_info %]
	<tr>
	  <td class="image"></td>
	  <td>[% s.name %]</td>
	</tr>
      [% END %]
      </table>
      [% END %]

      [% IF nproc gt 0 %] 
      <h2 id="proc">[% nproc %] procedure[% IF nproc gt 1 %]s[% END %]</h2>
      [% END %]

      [% IF nfunc gt 0 %] 
      <h2 id="func">[% nfunc %] function[% IF nfunc gt 1 %]s[% END %]</h2>
      [% END %]

      [% IF nobj gt 0 %] 
      <h2 id="obj">[% nobj %] object[% IF nobj gt 1 %]s[% END %]</h2>
      <p class="sav_warning">Objects not referenced through a local variable are not displayed.</p>
      [% END %]

      [% IF nptr gt 0 %] 
      <h2 id="ptr">[% nptr %] pointer[% IF nptr gt 1 %]s[% END %]</h2>
      <p class="sav_warning">Pointers not referenced through a local variable are not displayed.</p>
      [% END %]

      [% IF nstructdef gt 0 %] 
      <h2 id="structdef">[% nstructdef %] structure definition[% IF nstructdef gt 1 %]s[% END %]</h2>
      <table class="variables">
      [% FOREACH s IN structdef_info %]
	<tr>
	  <td class="image"></td>
	  <td>[% s.name %]</td>
	</tr>
      [% END %]
      </table>
      [% END %]

      [% IF footer NE '' %]<div id="footer">[% INCLUDE footer %]</div>[% END %]
      
      [% INCLUDE_TEMPLATE tagline_filename %]
      
    </div>
    
  </body>
</html>