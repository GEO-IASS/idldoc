
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<!-- Generated by IDLdoc [% version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <title>[% directory %] ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
         [% INCLUDE css_location %]
    </style>	
    <style type="text/css" media="print">
         [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" href="[% root %]main_files.css" media="all"/>
    <link rel="stylesheet" type="text/css" href="[% root %]main_files_print.css" media="print"/>
    [% END %]

    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% directory %] ([% title %])";
      }
    </script>
  </head>


  <body onload="setTitle();">
    
    [% INCLUDE_TEMPLATE navbar_filename %]
    
    <div id="container">

      <h1>Directory overview for [% directory %]</h1>
 
      [% comments %]

      [% IF nprofiles gt 0 %]
      <h2>File summary</h2>

      <dl>
      [% FOREACH p in profiles %]
	<dt><a href="[% p.url %]">[% p.name %]</a></dt>
	<dd>[% p.comment %]</dd>
      [% END %]
      </dl>
      [% END %]

      [% IF nsavfiles gt 0 %]
      <h2>SAV files</h2>
      
      <dl>
	[% FOREACH s IN savfiles %]
	<dt><a href="[% s.url %]" target="file_frame">[% s.name %]</a></dt>[% END %]
      </dl>
      [% END %]

      [% IF footer NE '' %]<div id="footer">[% INCLUDE footer %]</div>[% END %]

      [% INCLUDE_TEMPLATE tagline_filename %]
      
    </div>
    
  </body>
</html>