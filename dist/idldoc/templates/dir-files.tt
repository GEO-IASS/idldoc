
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">


<!-- Generated by IDLdoc [% version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <title>[% dirname %]</title>
    [% IF embed %]
    <style type="text/css">
      [% INCLUDE css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" href="[% root %]listings.css" />
    [% END %]
  </head>

  <body>
    <div id="container">
      <h1><a href="[% dirurl %]" target="file_frame">[% dirname %]</a></h1>

      [% IF nfiles gt 0 %]
      <ul>
	[% FOREACH file IN files %]
	<li><a href="[% file.url %]" target="file_frame">[% file.name %]</a></li>[% END %]
      </ul>
      
      <div id="tagline">[% strtrim(nfiles, 2) %] file[% nfiles gt 1 ? "s" : "" %]</div>
      [% END %]

      [% IF nsavfiles gt 0 %]
      <h1>SAV files</h1>
      
      <ul>
	[% FOREACH s IN savfiles %]
	<li><a href="[% s.url %]" target="file_frame">[% s.name %]</a></li>[% END %]
      </ul>

      <div id="tagline">[% strtrim(nsavfiles, 2) %] file[% nsavfiles gt 1 ? "s" : "" %]</div>
      
      [% END %]
    </div>
  </body>
</html>
