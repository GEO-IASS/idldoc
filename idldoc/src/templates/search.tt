 				<form name="basicForm">
 					<input class="text" name="basicText" size="50"/> &nbsp; <input name="send" type="submit" value="Search" onClick="basicsearch()"/>
 				</form>
 
    	
    	<h2>Advanced search</h2>

			<form name="advancedForm">
			  <table summary="Advanced search">
			    <tr>
			      <td>Routine name</td>
				    <td><input class="text" name="routineName" size="30"/> </td>
				  </tr>

			    <tr>
			      <td>Parameter name</td>
				    <td><input class="text" name="paramName" size="30"/> </td>
				  </tr>

			    <tr>
			      <td>Code header</td>
				    <td><input class="text" name="codeHeader" size="30"/> </td>
				  </tr>
				  				  
          <tr>
            <td></td>            
			      <td><input name="send" type="submit" value="Search" onClick="javascript:advancedsearch()"/></td>
			    </tr>

				</table>
			</form>

			<br/>
			
    	[% IF footer ne '' %][% INCLUDE_TEMPLATE footer %][% END %]    		
    </div>    

    [% INCLUDE_TEMPLATE idldoc_footer_location %]
