<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <title>[% basename %] ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
         [% INCLUDE css_location %]
    </style>
    <style type="text/css" media="print">
         [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all" 
          href="[% relative_root %]idldoc-resources/main.css" />
    <link rel="stylesheet" type="text/css" media="print" 
          href="[% relative_root %]idldoc-resources/main-print.css" />
    [% END %]
    
    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% basename %] ([% title %])";
      }
    </script>
  </head>
    
  <body onload="setTitle();" id="root">

    [% INCLUDE_TEMPLATE idldoc_header_location %]
    
    <div class="content">
      <h2 class="directory">[% location %]</h2>
      <h1 class="basename">[% basename %]</h1>
      
      [% IF is_batch %]<span class="file_attribute small">batch file</span>[% END %]
      [% IF has_main_level %]<span class="file_attribute small">includes main-level program</span>[% END %]
			
      [% IF has_comments %]
      <div id="file_comments">
      	<p [% IF preformat %]class="pre"[% END %]>[% comments %]</p>
      </div>
      [% END %]

      [% IF is_class %][% SCOPE class %]
      <h2>Class description</h2>
      
      [% IF n_ancestors gt 0 %]
      <h3>Inheritance</h3>
      	<ul>
      		[% FOREACH a IN ancestors %][% SCOPE a %]
      		<li><a href="[% url %]">[% classname %]</a></li>
      		[% END %][% END %]
      	</ul>
      [% END %]
      
      <h3>Fields</h3>
      
      [% END %][% END %]
            
      [% IF n_routines gt 1 %]
      <div>
        <h2>Routines</h2>
      
        <dl class="routine-summary">
          [% FOREACH r IN routines %][% SCOPE r %]
            <dt class="syntax"><a href="#[% name %]"><code>[% IF is_function %]result = [% END %][% name %][% IF is_function %]([% END %][% IF n_parameters gt 0 %][% FOREACH p IN parameters %][% SCOPE p %][% prefix %][% name %][% suffix %][% END %][% END %][% END %][% IF n_keywords gt 0 %][% FOREACH k IN keywords %][% SCOPE k %][% prefix %][% name %]=[% name %][% suffix %][% END %][% END %][% END %][% IF is_function %])[% END %]</code></a></dt>
            <dd class="smaller">[% comments_first_line %]</dd>
          [% END %][% END %]
        </dl>
      </div>
      [% END %]
      
    	
    	[% IF n_routines gt 0 %]
    	<div id="routine-details">
    	  [% IF n_routines gt 1 %]<h2>Routine details</h2>[% END %]
        
        [% FOREACH r IN routines %][% SCOPE r %]
        <div class="routine-details" id="[% name %]">
        	<h3><a href="#root" class="top smallest">top</a>[% name %]</h3>
        	
          <p class="syntax"><code>[% IF is_function %]result = [% END %][% name %][% IF is_function %]([% END %][% IF n_parameters gt 0 %][% FOREACH p IN parameters %][% SCOPE p %][% prefix %][% name %][% suffix %][% END %][% END %][% END %][% IF n_keywords gt 0 %][% FOREACH k IN keywords %][% SCOPE k %][% prefix %][% name %]=[% name %][% suffix %][% END %][% END %][% END %][% IF is_function %])[% END %]</code></p>
          
          [% IF has_comments %]
          <p [% IF preformat %]class="pre"[% END %]>[% comments %]</p>
          [% END %]
          
          [% IF is_function %]
          <h4>Return value</h4>
          [% END %]
          
          [% IF n_parameters gt 0 %]
          <h4>Parameters</h4>
            <dl>
            [% FOREACH p IN parameters %][% SCOPE p %]
              <dt>[% name %] <span class="small"></span></dt>
              <dd></dd>
            [% END %][% END %]
            </dl>
          [% END %]
          
          [% IF n_keywords gt 0 %]
          <h4>Keywords</h4>
            <dl>
            [% FOREACH k IN keywords %][% SCOPE k %]
              <dt>[% name %] <span class="small"></span></dt>
              <dd></dd>
            [% END %][% END %]
            </dl>
          [% END %]
          
          [% IF statistics %]
          <h4>Statistics</h4>
          [% END %]
          
          <h4>Other attributes</h4>
        </div>            
        [% END %][% END %]
            
      </div>
      [% END %]
      
      <div id="attributes">
        <h2>File attributes</h2>
        	
        <table class="attribute smaller">
          <tr>
            <td class="name">Modifcation date:</td>
            <td>[% modification_time %]</td>
          </tr>

          <tr>
            <td class="name">Lines:</td>
            <td>[% n_lines %]</td>
          </tr>
                            
          <tr>
            <td class="name">Docformat:</td>
            <td>[% format %][% IF format ne '' %] [% END %][% markup %]</td>
          </tr>
        </table> 
      </div>
      
    </div>
    
    [% INCLUDE_TEMPLATE idldoc_footer_location %]    
  </body>
</html>