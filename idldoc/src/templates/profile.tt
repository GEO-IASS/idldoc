<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <title>[% basename %] ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
         [% INCLUDE css_location %]
    </style>
    <style type="text/css" media="print">
         [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all" 
          href="[% relative_root %]idldoc-resources/main.css" />
    <link rel="stylesheet" type="text/css" media="print" 
          href="[% relative_root %]idldoc-resources/main-print.css" />
    [% END %]
    
    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% basename %] ([% title %])";
      }
    </script>
  </head>
    
  <body onload="setTitle();">

    [% INCLUDE_TEMPLATE idldoc_header_location %]
    
    <div class="content">
      <h2 class="directory">[% location %]</h2>
      <h1 class="basename">[% basename %]</h1>
    
      [% IF is_batch %]Batch file[% END %]
      [% IF has_main_level %]includes main-level program[% END %]

      [% IF n_routines gt 1 %]
        <dl class="routine-summary">
          [% FOREACH r IN routines %][% SCOPE r %]
            <dt class="syntax"><code>[% IF is_function %]result = [% END %][% name %][% IF n_parameters gt 0 %][% FOREACH p IN parameters %][% SCOPE p %][% prefix %][% name %][% suffix %][% END %][% END %][% END %][% IF n_keywords gt 0 %][% FOREACH k IN keywords %][% SCOPE k %][% prefix %][% name %]=[% name %][% suffix %][% END %][% END %][% END %]</code></dt>
            <dd class="small">Comments go here.</dd>
          [% END %][% END %]
        </dl>
      [% END %]
      
      [% IF is_class %]
    	<h2>Class description</h2>
    	[% END %]
    	
    	[% IF n_routines gt 0 %]
    	<div id="routine-details">
    	  [% IF n_routines gt 1 %]<h2>Routine details</h2>[% END %]
        
        [% FOREACH r IN routines %][% SCOPE r %]
        <div class="routine-detail">
          <p class="syntax"><code>[% IF is_function %]result = [% END %][% name %][% IF n_parameters gt 0 %][% FOREACH p IN parameters %][% SCOPE p %][% prefix %][% name %][% suffix %][% END %][% END %][% END %][% IF n_keywords gt 0 %][% FOREACH k IN keywords %][% SCOPE k %][% prefix %][% name %]=[% name %][% suffix %][% END %][% END %][% END %]</code></p>
        </div>            
        [% END %][% END %]
            
      </div>
      [% END %]
      
      <div id="attributes">
        <h2>File attributes</h2>
        	
        <table class="attribute smaller">
          <tr>
            <td class="name">Modifcation date:</td>
            <td>[% modification_time %]</td>
          </tr>

          <tr>
            <td class="name">Lines:</td>
            <td>[% n_lines %]</td>
          </tr>
                            
          <tr>
            <td class="name">Docformat:</td>
            <td>[% format %][% IF format ne '' %] [% END %][% markup %]</td>
          </tr>
        </table> 
      </div>
      
    </div>
    
    [% INCLUDE_TEMPLATE idldoc_footer_location %]    
  </body>
</html>