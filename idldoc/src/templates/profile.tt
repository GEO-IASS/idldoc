<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <title>[% basename %] ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
         [% INCLUDE css_location %]
    </style>
    <style type="text/css" media="print">
         [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all" 
          href="[% relative_root %]idldoc-resources/main.css" />
    <link rel="stylesheet" type="text/css" media="print" 
          href="[% relative_root %]idldoc-resources/main-print.css" />
    [% END %]
    
    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% basename %] ([% title %])";
      }
    </script>
  </head>
    
  <body onload="setTitle();" id="root">

    [% INCLUDE_TEMPLATE idldoc_header_location %]
    
    <div class="content">
      <h2 class="directory">[% location %]</h2>
      <h1 class="basename">[% basename %]</h1>
      
      [% IF is_batch %]<span class="file_attribute small">batch file</span>[% END %]
      [% IF has_main_level %]<span class="file_attribute small">includes main-level program</span>[% END %]
			
      [% IF has_comments %]
      <div id="file_comments">
      	<div class="small [% IF preformat %]pre[% END %]">[% comments %]</div>
      </div>
      [% END %]

      [% IF is_class %][% SCOPE class %]
      <h2>Class description</h2>
      
      [% IF n_ancestors gt 0 %]
      <h3>Inheritance</h3>
        <ul>
          [% FOREACH p IN parents %]
          <li>[% IF p->hasUrl() %]<a href="[% relative_root %][% p->getUrl() %]">[% END %][% p->getClassname() %][% IF p->hasUrl() %]</a>[% END %]</li>
          [% END %]
        </ul>
        
        [% IF n_ancestors gt n_parents %]      
        <p class="smaller">All ancestors:
      		[% FOREACH a IN ancestors %]
      		[% IF a->hasUrl() %]<a href="[% relative_root %][% a->getUrl() %]">[% END %][% a->getClassname() %][% IF a->hasUrl() %]</a>[% END %]
      		[% END %]
      	</p>
      	[% END %]
      [% END %]
      
      <h3>Fields</h3>
      
      [% END %][% END %]
            
      [% IF n_routines gt 1 %]
      <div>
        <h2>Routines</h2>
      
        <dl class="routine-summary">
          [% FOREACH r IN routines %][% SCOPE r %]
            <dt class="syntax"><a href="#[% name %]"><code>[% IF is_function %]<span class="var">result</span> = [% END %][% name %][% IF is_function %]([% END %][% IF n_parameters gt 0 %][% FOREACH p IN parameters %][% SCOPE p %]<span class="argument">[% IF is_optional %] [[% END %][% prefix %][% name %][% suffix %][% IF is_optional %]][% END %]</span>[% END %][% END %][% END %][% IF n_keywords gt 0 %][% FOREACH k IN keywords %][% SCOPE k %]<span class="argument">[% IF is_optional %] [[% END %][% prefix %][% IF is_boolean %]/[% END %][% name %][% IF ~is_boolean %]=<span class="var">[% IF type ne '' %][% type %][% ELSE %][% name %][% END %]</span>[% END %][% suffix %][% IF is_optional %]][% END %]</span>[% END %][% END %][% END %][% IF is_function %])[% END %]</code></a></dt>
            <dd class="small">[% comments_first_line %]</dd>
          [% END %][% END %]
        </dl>
      </div>
      [% END %]
      
    	
    	[% IF n_routines gt 0 %]
    	<div id="routine-details">
    	  [% IF n_routines gt 1 %]<h2>Routine details</h2>[% END %]
        
        [% FOREACH r IN routines %][% SCOPE r %]
        <div class="routine-details" id="[% name %]">
        	<h2 class="routine-name"><a href="#root" class="top">top</a>[% name %]</h2>
        	
          <p class="syntax"><code>[% IF is_function %]<span class="var">result</span> = [% END %][% name %][% IF is_function %]([% END %][% IF n_parameters gt 0 %][% FOREACH p IN parameters %][% SCOPE p %]<span class="argument">[% IF is_optional %] [[% END %][% prefix %]<a href="#[% id %]">[% name %]</a>[% suffix %][% IF is_optional %]][% END %]</span>[% END %][% END %][% END %][% IF n_keywords gt 0 %][% FOREACH k IN keywords %][% SCOPE k %]<span class="argument">[% IF is_optional %] [[% END %][% prefix %][% IF is_boolean %]/[% END %]<a href="#[% id %]">[% name %]</a>[% IF ~is_boolean %]=<span class="var">[% IF type ne '' %][% type %][% ELSE %][% name %][% END %]</span>[% END %][% suffix %][% IF is_optional %]][% END %]</span>[% END %][% END %][% END %][% IF is_function %])[% END %]</code></p>
          
          [% IF has_comments %]
          <div class="small [% IF preformat %]pre[% END %]">[% comments %]</div>
          [% END %]
          
          [% IF is_function and has_returns %]
          <h3>Return value</h3>
          	<div class="small">[% returns %]</div>
          [% END %]
          
          [% IF n_parameters gt 0 %]
          <h3>Parameters</h3>
            <dl>
            [% FOREACH p IN parameters %][% SCOPE p %]
              <dt id="[% id %]">[% name %] <span class="smaller">[% IF is_input %]in [% END %][% IF is_output %]out [% END %][% IF is_optional %]optional [% END %][% IF is_required %]required [% END %][% IF is_private %]private [% END %][% IF is_hidden %]hidden [% END %][% IF is_obsolete %]obsolete [% END %][% IF type ne '' %]type=[% type %] [% END %][% IF default_value ne '' %]default=[% default_value %][% END %]</span></dt>
              <dd class="small">[% comments %]</dd>
            [% END %][% END %]
            </dl>
          [% END %]
          
          [% IF n_keywords gt 0 %]
          <h3>Keywords</h3>
            <dl>
            [% FOREACH k IN keywords %][% SCOPE k %]
              <dt id="[% id %]">[% name %] <span class="smaller">[% IF is_input %]in [% END %][% IF is_output %]out [% END %][% IF is_optional %]optional [% END %][% IF is_required %]required [% END %][% IF is_private %]private [% END %][% IF is_hidden %]hidden [% END %][% IF is_obsolete %]obsolete [% END %][% IF type ne '' %]type=[% type %] [% END %][% IF default_value ne '' %]default=[% default_value %][% END %]</span></dt>
              <dd class="small">[% comments %]</dd>
            [% END %][% END %]
            </dl>
          [% END %]
          
          [% IF has_others %]
          <h3>Other attributes</h3>

            <table class="attribute small">
              [% IF has_bugs %]
              <tr>
                <td class="name">Bugs:</td>
                <td>[% bugs %]</td>
              </tr>          
              [% END %]

              [% IF has_pre %]
              <tr>
                <td class="name">Pre-condition:</td>
                <td>[% pre %]</td>
              </tr>          
              [% END %]

              [% IF has_post %]
              <tr>
                <td class="name">Post-condition:</td>
                <td>[% post %]</td>
              </tr>          
              [% END %]                            
            </table>
          [% END %]
          
          [% IF statistics %]
          <h3>Statistics</h3>

		        <table class="attribute small">
		          <tr>
		            <td class="name">Lines:</td>
		            <td></td>
		          </tr> 

		          <tr>
		            <td class="name">McCabe complexity:</td>
		            <td></td>
		          </tr>   		            		                
		        </table>
          [% END %]
                    
        </div>            
        [% END %][% END %]
            
      </div>
      [% END %]
      
      <div id="attributes">
        <h2>File attributes</h2>
        	
        <table class="attribute smaller">
          <tr>
            <td class="name">Modifcation date:</td>
            <td>[% modification_time %]</td>
          </tr>

          <tr>
            <td class="name">Lines:</td>
            <td>[% n_lines %]</td>
          </tr>
                            
          <tr>
            <td class="name">Docformat:</td>
            <td>[% format %][% IF format ne '' %] [% END %][% markup %]</td>
          </tr>
        </table> 
      </div>
      
      [% IF footer ne '' %][% INCLUDE_TEMPLATE footer %][% END %]
    </div>
    
    [% INCLUDE_TEMPLATE idldoc_footer_location %]    
  </body>
</html>