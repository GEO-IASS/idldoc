<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <title>[% basename %] ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
         [% INCLUDE css_location %]
    </style>
    <style type="text/css" media="print">
         [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all" 
          href="[% relative_root %]idldoc-resources/main.css" />
    <link rel="stylesheet" type="text/css" media="print" 
          href="[% relative_root %]idldoc-resources/main-print.css" />
    [% END %]
    
    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% basename %] ([% title %])";
      }
    </script>
  </head>
    
  <body onload="setTitle();">

    [% INCLUDE_TEMPLATE idldoc_header_location %]
    
    <div class="content">
      
      <h2>SAV file attributes</h2>
      
      <table class="attribute">
        <tr>
          <td class="name">Filename:</td>
          <td>[% filename %]</td>
        </tr>     
        <tr>
          <td class="name">Description:</td>
          <td>[% description %]</td>
        </tr>    
        <tr>
          <td class="name">SAV file type:</td>
          <td>[% filetype %]</td>
        </tr>
        <tr>
          <td class="name">Creator: </td>
          <td>[% user %]@[% host %] on [% os %]/[% arch %]</td>
        </tr>
        <tr>
          <td class="name">Creation date: </td>
          <td>[% creation_date %]</td>
        </tr>      
        <tr>
          <td class="name">IDL version: </td>
          <td>[% release %]</td>
        </tr>    
      </table>

      <h2>Contents</h2>
      
      <table class="attribute">      
        <tr>
          <td class="name">Procedures: </td>
          <td>[% n_procedure %]</td>
        </tr>  
        <tr>
          <td class="name">Functions: </td>
          <td>[% n_function %]</td>
        </tr> 
        <tr>
          <td class="name">Variables: </td>
          <td>[% n_var %]</td>
        </tr>  
        <tr>
          <td class="name">System variables: </td>
          <td>[% n_sysvar %]</td>
        </tr>  
        <tr>
          <td class="name">Common blocks: </td>
          <td>[% n_common %]</td>
        </tr>
        <tr>
          <td class="name">Structure definitions: </td>
          <td>[% n_structdef %]</td>
        </tr>  
        <tr>
          <td class="name">Pointers: </td>
          <td>[% n_pointer_heapvar %]</td>
        </tr>                            
        <tr>
          <td class="name">Objects: </td>
          <td>[% n_object_heapvar %]</td>
        </tr>  
      </table>
      
      <h2>File attributes</h2>
      
    </div>
    
    [% INCLUDE_TEMPLATE idldoc_footer_location %]    
  </body>
 </html>